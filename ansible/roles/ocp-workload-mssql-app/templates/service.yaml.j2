---
apiVersion: v1
kind: Service
metadata:
 name: {{ mssql_app_service_name }}
 namespace: {{ mssql_app_namespace }}
spec:
 selector:
   app: {{ mssql_app_service_selector }}
 ports:
   - protocol: TCP
     nodePort: {{ mssql_app_service_port }}
     port: 3000
     targetPort: 3000
 type: LoadBalancer
